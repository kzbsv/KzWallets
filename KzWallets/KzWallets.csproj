<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <LangVersion>8.0</LangVersion>
    <Version>0.0.1</Version>
    <Description>Bitcoin SV Wallets</Description>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <RepositoryUrl>https://github.com/kzbsv/KzWallets</RepositoryUrl>
    <PackageTags>Bitcoin SV BSV Wallet</PackageTags>
    <Authors>TonesNotes</Authors>
    <Company>kzbsv.org</Company>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <Optimize>true</Optimize>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="KzBsv" Version="0.1.6" />
  </ItemGroup>

</Project>
